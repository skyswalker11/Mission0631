@{
    ViewData["Task"] = "Task Application";
}

@model mission6.Models.ApplicationResponse



<div>
    <form method="post">
        <label asp-for="Quadrant">Quadrant:</label>

        <select asp-for="Quadrant">
            <option value="          ">           </option>
            <option value="Importan & Urgent">Importan & Urgent</option>
            <option value="Important & Non-Urgent">Important & Non-Urgent</option>
            <option value="Unimportant & Urgent">Unimportant & Urgent</option>
            <option value="Unimportant & Non-Urgent">Unimportant & Non-Urgent</option>

        </select>
        <br />
        <label asp-for="Category">Category:</label>

        <select asp-for="Category">
            <option value="          ">           </option>
            <option value="Home">Home</option>
            <option value="School">School</option>
            <option value="Work">Work</option>
            <option value="Chruch">Church</option>

        </select>
        <br />
      

        <label asp-for="Task">Task:</label>
        <input asp-for="Task" /><br />
      
      

        <label asp-for="Due_Date">Due_Date</label>
        <input required asp-for="Due_Date" /><br />

        <label asp-for="Completed">Completed:</label>
        <select asp-for="Completed">
            <option value="">Please Select</option>
            <option value="Yes">Yes</option>
            <option value="No">No</option>
        </select><br />


        <button type="submit">Submit Information</button>
        <div asp-validation-summary="All">

        </div>
    </form>
</div>